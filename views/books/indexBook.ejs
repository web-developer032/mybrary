<h2 class="page-header">Search Book</h2>
<form action="/books" method="GET">
  <div class="form-row">
    <div class="form-item">
      <label for="title">Title</label>
      <input
        type="text"
        name="title"
        id="title"
        value="<%= searchOptions.title %>"
      />
    </div>
  </div>

  <div class="form-row">
    <div class="form-item">
      <label for="publishedAfter">Published After</label>
      <input
        type="date"
        name="publishedAfter"
        id="publishedAfter"
        value="<%= searchOptions.publishedAfter %>"
      />
    </div>

    <div class="form-item">
      <label for="publishedBefore">Published Before</label>
      <input
        type="date"
        name="publishedBefore"
        id="publishedBefore"
        value="<%= searchOptions.publishedBefore %>"
      />
    </div>
  </div>
  <div class="form-row form-row-end">
    <button class="btn btn-primary">Search</button>
  </div>
</form>

<br />

<div><%- include("../partials/bookGrid",{books,large:false}) %></div>
