<h2>Search Book</h2>
<form action="/books" method="GET">
  <div>
    <label for="title">Title</label>
    <input
      type="text"
      name="title"
      id="title"
      value="<%= searchOptions.title %>"
    />
  </div>
  <div>
    <label for="publishedAfter">Published After</label>
    <input
      type="date"
      name="publishedAfter"
      id="publishedAfter"
      value="<%= searchOptions.publishedAfter %>"
    />
  </div>
  <div>
    <label for="publishedBefore">Published Before</label>
    <input
      type="date"
      name="publishedBefore"
      id="publishedBefore"
      value="<%= searchOptions.publishedBefore %>"
    />
  </div>

  <button>Search</button>
</form>

<br />
<% books.forEach(book => { %>
<a href="/books/<%= book.id %>">
  <img src="<%= book.coverImagePath %>" alt="" height="150" width="100" />
</a>

<% }) %>
